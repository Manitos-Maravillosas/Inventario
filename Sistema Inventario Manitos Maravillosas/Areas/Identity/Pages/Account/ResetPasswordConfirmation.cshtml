@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Confirmación de reestablecimiento de contraseña";
}
<div class="row justify-content-center login-card">
    <div class="col-md-4 w-100 text-center">
        <h1>@ViewData["Title"]</h1>
        <p>
            Tu contraseña ha sido restablecida. Por favor haz <a asp-page="./Login">click aquí para ingresar</a>.
        </p>
    </div>
</div>
